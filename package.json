{
  "name": "premid",
  "productName": "PreMiD",
  "description": "Discord Rich Presence for websites.",
  "version": "2.0.2",
  "repository": "https://github.com/PreMiD/PreMiD",
  "scripts": {
    "init": "npm i && tsc && tsc pkg devMode && node devMode.js compile",
    "start": "electron dist/app/.",
    "dev": "node devMode.js",
    "pkg": "rimraf dist installer_assets/updater.app && tsc && node devMode compile && cd dist/app/ && npm i && cd ../../ && node pkg",
    "pkgdist": "npm run pkg && node compile",
    "pkgwin32": "electron-packager ./src/ --out=./dist/ --overwrite --icon=./installer_assets/appIcon.ico --platform=win32 --arch=ia32",
    "pkglinux": "electron-packager ./src/ --out=./dist/ --overwrite --icon=./appIcon.ico --asar --platform=linux --arch=x64"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/auto-launch": "^5.0.1",
    "@types/chalk": "^2.2.0",
    "@types/discord-rpc": "^3.0.1",
    "@types/electron-packager": "^14.0.0",
    "@types/fs-extra": "^8.0.0",
    "@types/ini": "^1.3.30",
    "@types/request-promise-native": "^1.0.16",
    "@types/socket.io": "^2.1.2",
    "chalk": "^2.4.2",
    "electron": "^6.0.10",
    "electron-packager": "^14.0.6",
    "fast-glob": "^3.0.4",
    "fs-extra": "^8.1.0",
    "ini": "^1.3.5",
    "nodemon": "^1.19.2",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.13",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "discord-rpc": "github:discordjs/RPC",
    "electron-store": "^5.0.0",
    "socket.io": "^2.3.0",
    "sudo-prompt": "^9.0.0"
  }
}

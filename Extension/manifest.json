{
  "name": "YT Presence",
  "author": "Timeraa",
  "version": "1.0",
  "description": "YT Presence adds cool Features to YouTube/YouTube Music. Like Discord Rich Presence integration, Media controls and much more.",
  "manifest_version": 2,
  "icons": {
    "2024": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "2024": "icon.png"
    }
  },
  "content_scripts": [
    {
      "js": ["util/jquery-3.3.1.min.js", "util/socket.io-2.1.1.min.js"],
      "css": ["connect.css"],
      "matches": [
        "https://www.youtube.com/*",
        "https://music.youtube.com/*",
        "https://www.netflix.com/*"
      ]
    },
    {
      "js": ["presences/YTM.js"],
      "matches": ["https://music.youtube.com/*"]
    },
    {
      "js": ["presences/YT.js"],
      "matches": ["https://www.youtube.com/*"]
    },
    {
      "js": ["presences/Netflix.js"],
      "matches": ["https://www.netflix.com/*"]
    }
  ],
  "permissions": ["http://localhost:3000/", "tabs", "storage"]
}

{
  "name": "PreMiD",
  "author": "Timeraa & Fruxh",
  "version": "1.2.50",
  "manifest_version": 2,
  "web_accessible_resources": ["icon.png", "languages/*.json"],
  "icons": {
    "2024": "icon.png"
  },
  "background": {
    "scripts": [
      "js/util/socket.io-2.1.1.min.js",
      "js/util/presence.js",
      "js/background.js"
    ],
    "presistent": false
  },
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "2024": "icon.png"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "js/util/jquery-3.3.1.min.js",
        "js/util/socket.io-2.1.1.min.js",
        "js/util/socketConnector.js",
        "js/util/custom-i18n.js"
      ],
      "css": ["css/connect.css"],
      "matches": ["<all_urls>"]
    },
    {
      "js": ["js/presences/YouTubeMusic.js"],
      "matches": ["*://music.youtube.com/*"]
    },
    {
      "js": ["js/presences/YouTube.js"],
      "matches": ["*://www.youtube.com/*"]
    },
    {
      "js": ["js/presences/Netflix.js"],
      "matches": ["*://*.netflix.com/*"]
    },
    {
      "js": ["js/presences/SoundCloud.js"],
      "matches": ["*://*.soundcloud.com/*"]
    },
    {
      "js": ["js/presences/Twitch.js"],
      "matches": ["*://*.twitch.tv/*"]
    },
    {
      "js": ["js/presences/KissAnime.js"],
      "matches": ["*://kissanime.ac/*", "*://kissanime.ru/*"]
    },
    {
      "js": ["js/presences/JKAnime.js"],
      "matches": ["*://jkanime.net/*"]
    },
    {
      "js": ["js/presences/FimFiction.js"],
      "matches": ["*://www.fimfiction.net/*"]
    },
    {
      "js": ["js/presences/crunchyroll.js"],
      "matches": ["*://www.crunchyroll.com/*"]
    },
    {
      "js": ["js/presences/HentaiHaven.js"],
      "matches": ["*://hentaihaven.org/*"]
    },
    {
      "js": ["js/presences/Rabbit.js"],
      "matches": ["*://www.rabb.it/*"]
    },
    {
      "js": ["js/presences/MasterAnime.js"],
      "matches": ["*://www.masterani.me/*"]
    },
    {
      "js": ["js/presences/SuperAnimes.js"],
      "matches": ["*://www.superanimes.site/*"]
    },
    {
      "js": ["js/presences/9Anime.js"],
      "matches": ["*://www1.9anime.to/*"]
    },
    {
      "js": ["js/presences/Pandora.js"],
      "matches": ["*://www.pandora.com/*"]
    },
    {
      "js": ["js/presences/HentaiGasm.js"],
      "matches": ["*://hentaigasm.com/*"]
    },
    {
      "js": ["js/presences/Pokyun.js"],
      "matches": ["*://www.pokyun.tv/*"]
    },
    {
      "js": ["js/presences/Anime4You.js"],
      "matches": ["*://www.anime4you.one/*"]
    },
    {
      "matches": ["<all_urls>"],
      "js": [
        "js/util/jquery-3.3.1.min.js",
        "js/iframeWitch/Anime4You_Witch.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "http://localhost:3020/",
    "tabs",
    "storage",
    "declarativeContent"
  ]
}
